# Read the number of test cases
T = int(input())

# Loop through each test case
for _ in range(T):
    # Read the angles A, B, C, and D
    A, B, C, D = map(int, input().split())
    
    # Check if the quadrilateral is cyclic
    if (A + C == 180 or B + D == 180):
        print("YES")
    else:
        print("NO")
