# Read number of test cases
T = int(input())

# Iterate over each test case
for _ in range(T):
    # Read the tastiness of the ingredients a, b, c, d
    a, b, c, d = map(int, input().split())
    
    # Calculate the maximum possible tastiness
    max_tastiness = max(a + c, a + d, b + c, b + d)
    
    # Output the result
    print(max_tastiness)
