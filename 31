# Read the number of test cases
T = int(input().strip())

# Process each test case
results = []
for _ in range(T):
    # Read the value of N for this test case
    N = int(input().strip())
    # Calculate the number of ways to choose a captain and vice-captain
    result = N * (N - 1)
    # Store the result
    results.append(result)

# Output all results, one per line
for result in results:
    print(result)
